{
    "version": "1.0.2",
    "summary": "A library for constructing and applying scales. ",
    "repository": "https://github.com/enetsee/elm-scale.git",
    "license": "BSD3",
    "source-directories": [
        "src"
    ],
    "exposed-modules": [
        "Scale.Config",
        "Scale.Continuous",
        "Scale.Continuous.Linear",
        "Scale.Continuous.Log",
        "Scale.Continuous.Power",
        "Scale.Ordinal",
